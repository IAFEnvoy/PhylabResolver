{
    "name": "鼠大物理基础题自动作答",
    "version": "0.1",
    "manifest_version": 3,
    "description": "从此，鼠鼠们脱离了物理基础题",
    "author": "IAFEnvoy",
    "icons": {
        "16": "icons/16x16.png",
        "32": "icons/32x32.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "js": [
                "js/resolver.js"
            ],
            "matches": [
                "http://pmedia.shu.edu.cn/xiti/index.aspx"
            ],
            "run_at": "document_start"
        }
    ]
}